{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "imports": {
      "type": "array",
      "items": {}
    },
    "search_files_in_root": {
      "type": "boolean"
    },
    "escape_key_behavior": {
      "type": "string"
    },
    "close_on_focus_loss": {
      "type": "boolean"
    },
    "consider_preedit": {
      "type": "boolean"
    },
    "pop_to_root_on_close": {
      "type": "boolean"
    },
    "favicon_service": {
      "type": "string"
    },
    "keybinding": {
      "type": "string"
    },
    "font": {
      "type": "object",
      "properties": {
        "normal": {
          "type": "object",
          "properties": {
            "family": {
              "type": "string"
            },
            "size": {
              "type": "number"
            }
          }
        }
      }
    },
    "theme": {
      "type": "object",
      "properties": {
        "light": {
          "type": "object",
          "properties": {
            "name": {
              "type": "string"
            },
            "icon_theme": {
              "type": "string"
            }
          }
        },
        "dark": {
          "type": "object",
          "properties": {
            "name": {
              "type": "string"
            },
            "icon_theme": {
              "type": "string"
            }
          }
        }
      }
    },
    "launcher_window": {
      "type": "object",
      "properties": {
        "opacity": {
          "type": "number"
        },
        "blur": {
          "type": "object",
          "properties": {
            "enabled": {
              "type": "boolean"
            }
          }
        },
        "dim_around": {
          "type": "boolean"
        },
        "client_side_decorations": {
          "type": "object",
          "properties": {
            "enabled": {
              "type": "boolean"
            },
            "rounding": {
              "type": "integer"
            },
            "border_width": {
              "type": "integer"
            }
          }
        },
        "compact_mode": {
          "type": "object",
          "properties": {
            "enabled": {
              "type": "boolean"
            }
          }
        },
        "size": {
          "type": "object",
          "properties": {
            "width": {
              "type": "integer"
            },
            "height": {
              "type": "integer"
            }
          }
        },
        "screen": {
          "type": "string"
        },
        "layer_shell": {
          "type": "object",
          "properties": {
            "enabled": {
              "type": "boolean"
            },
            "keyboard_interactivity": {
              "type": "string"
            },
            "layer": {
              "type": "string"
            }
          }
        }
      }
    },
    "keybinds": {
      "type": "object",
      "properties": {
        "open-search-filter": {
          "type": "string"
        },
        "open-settings": {
          "type": "string"
        },
        "toggle-action-panel": {
          "type": "string"
        },
        "action.copy": {
          "type": "string"
        },
        "action.copy-name": {
          "type": "string"
        },
        "action.copy-path": {
          "type": "string"
        },
        "action.dangerous-remove": {
          "type": "string"
        },
        "action.duplicate": {
          "type": "string"
        },
        "action.edit": {
          "type": "string"
        },
        "action.edit-secondary": {
          "type": "string"
        },
        "action.move-down": {
          "type": "string"
        },
        "action.move-up": {
          "type": "string"
        },
        "action.new": {
          "type": "string"
        },
        "action.open": {
          "type": "string"
        },
        "action.pin": {
          "type": "string"
        },
        "action.refresh": {
          "type": "string"
        },
        "action.remove": {
          "type": "string"
        },
        "action.save": {
          "type": "string"
        }
      }
    },
    "favorites": {
      "type": "array",
      "items": [
        {
          "type": "string"
        }
      ]
    },
    "fallbacks": {
      "type": "array",
      "items": [
        {
          "type": "string"
        }
      ]
    },
    "providers": {
      "type": "object"
    }
  }
}
